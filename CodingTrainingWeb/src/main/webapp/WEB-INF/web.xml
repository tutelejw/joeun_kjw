	</servlet>
    <servlet-mapping>
        <servlet-name>LoginUser</servlet-name> 
		<url-pattern>/LoginUser</url-pattern> 
	</servlet-mapping>
	        
		<servlet>
		<servlet-name>UserSelect</servlet-name> 
		<servlet-class>codingtrainingweb.UserSelect</servlet-class> 
	</servlet>
    <servlet-mapping>
